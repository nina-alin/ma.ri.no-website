{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:unicorn/recommended",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "simple-import-sort"
  ],
  "overrides": [
    {
      "files": [
        "*.tsx"
      ],
      "rules": {
        "unicorn/no-null": "off",
        "simple-import-sort/imports": "error",
        "simple-import-sort/exports": "error"
      },
      "excludedFiles": [
        "next.config.js",
        "next-env.d.ts"
      ]
    }
  ]
}
